var nowdays = new Date();&#xD;
    var year = nowdays.getFullYear();&#xD;
    var month = nowdays.getMonth();&#xD;
    if(month==0)&#xD;
    {&#xD;
        month=12;&#xD;
        year=year-1;&#xD;
    }&#xD;
    if (month &lt; 10) {&#xD;
