<?xml version="1.0" encoding="UTF-8"?>
<ViewConfig>
  <Arguments/>
  <Context/>
  <Model>
    <DataType name="dtCondition">
      <PropertyDef name="customerId">
        <Property></Property>
        <Property name="label">商家Id</Property>
      </PropertyDef>
      <PropertyDef name="customerName">
        <Property></Property>
        <Property name="label">商家</Property>
      </PropertyDef>
      <PropertyDef name="createYear">
        <Property name="label">年份</Property>
        <Property name="required">true</Property>
        <Property name="mapping">
          <Property name="mapValues">${dorado.getDataProvider(&quot;omsEnumPR#getReportMonthForYear&quot;).getResult()}</Property>
        </Property>
        <Property name="readOnly">true</Property>
      </PropertyDef>
      <PropertyDef name="createMonth">
        <Property name="label">月份</Property>
        <Property name="required">true</Property>
        <Property name="mapping">
          <Property name="mapValues">${dorado.getDataProvider(&quot;feesPayImportController#getAllMonth&quot;).getResult()}</Property>
        </Property>
        <Property name="readOnly">true</Property>
      </PropertyDef>
      <PropertyDef name="customerStatus">
        <Property></Property>
        <Property name="label">计算状态</Property>
        <Property name="mapping">
          <Property name="mapValues">
            <Collection>
              <Entity>
                <Property name="key">ALL</Property>
                <Property name="value">全部</Property>
              </Entity>
              <Entity>
                <Property name="key">1</Property>
                <Property name="value">成功</Property>
              </Entity>
              <Entity>
                <Property name="key">2</Property>
                <Property name="value">计算中</Property>
              </Entity>
              <Entity>
                <Property name="key">3</Property>
                <Property name="value">不成功</Property>
              </Entity>
            </Collection>
          </Property>
        </Property>
        <Property name="defaultValue">ALL</Property>
      </PropertyDef>
    </DataType>
    <DataType name="dtCustomerCalStatus">
      <Property name="creationType">com.jiuyescm.bms.asyn.vo.BmsCalcuTaskVo</Property>
      <PropertyDef name="creMonth">
        <Property></Property>
        <Property name="label">日期</Property>
      </PropertyDef>
      <PropertyDef name="customerId">
        <Property></Property>
        <Property name="label">商家Id</Property>
      </PropertyDef>
      <PropertyDef name="customerName">
        <Property></Property>
        <Property name="label">商家名称</Property>
      </PropertyDef>
      <PropertyDef name="customerStatus">
        <Property></Property>
        <Property name="label">计算状态</Property>
        <Property name="mapping">
          <Property name="mapValues">
            <Collection>
              <Entity>
                <Property name="key">1</Property>
                <Property name="value">成功</Property>
              </Entity>
              <Entity>
                <Property name="key">2</Property>
                <Property name="value">计算中</Property>
              </Entity>
              <Entity>
                <Property name="key">3</Property>
                <Property name="value">不成功</Property>
              </Entity>
            </Collection>
          </Property>
        </Property>
      </PropertyDef>
      <PropertyDef name="subjectNum">
        <Property></Property>
        <Property name="label">科目数</Property>
        <Property name="dataType">Integer</Property>
      </PropertyDef>
    </DataType>
    <DataType name="dtCustomerSubjectStatus">
      <Property name="creationType">com.jiuyescm.bms.asyn.vo.BmsCalcuTaskVo</Property>
      <PropertyDef name="subjectCode">
        <Property></Property>
      </PropertyDef>
      <PropertyDef name="subjectName">
        <Property></Property>
        <Property name="label">科目</Property>
      </PropertyDef>
      <PropertyDef name="customerStatus">
        <Property></Property>
        <Property name="label">计算状态</Property>
        <Property name="mapping">
          <Property name="mapValues">
            <Collection>
              <Entity>
                <Property name="key">1</Property>
                <Property name="value">成功</Property>
              </Entity>
              <Entity>
                <Property name="key">2</Property>
                <Property name="value">计算中</Property>
              </Entity>
              <Entity>
                <Property name="key">3</Property>
                <Property name="value">不成功</Property>
              </Entity>
            </Collection>
          </Property>
        </Property>
      </PropertyDef>
      <PropertyDef name="feesCount">
        <Property name="dataType">Integer</Property>
        <Property name="label">费用总数</Property>
      </PropertyDef>
      <PropertyDef name="finishCount">
        <Property name="dataType">Integer</Property>
        <Property name="label">计算成功</Property>
      </PropertyDef>
      <PropertyDef name="contractMissCount">
        <Property name="dataType">Integer</Property>
        <Property name="label">合同缺失</Property>
      </PropertyDef>
      <PropertyDef name="quoteMissCount">
        <Property name="dataType">Integer</Property>
        <Property name="label">报价缺失</Property>
      </PropertyDef>
      <PropertyDef name="noExeCount">
        <Property name="dataType">Integer</Property>
        <Property name="label">未计算</Property>
      </PropertyDef>
      <PropertyDef name="sysErrorCount">
        <Property name="dataType">Integer</Property>
        <Property name="label">系统错误</Property>
      </PropertyDef>
      <PropertyDef name="uncalcuCount">
        <Property name="dataType">Integer</Property>
        <Property name="label">不计算</Property>
      </PropertyDef>
      <PropertyDef name="feesType">
        <Property name="label">单位</Property>
      </PropertyDef>
    </DataType>
  </Model>
  <View>
    <ClientEvent name="onReady">var date=new Date;  &#xD;
var year=date.getFullYear();  &#xD;
var month=date.getMonth();&#xD;
if(month==0){&#xD;
	month=12;&#xD;
	year=year-1;&#xD;
}&#xD;
view.get(&quot;#queryForm.entity&quot;).set(&quot;createYear&quot;,year);&#xD;
view.get(&quot;#queryForm.entity&quot;).set(&quot;createMonth&quot;,month);</ClientEvent>
    <DataSet id="dsCondition">
      <ClientEvent name="onCreate">view.get(&quot;#dsCondition&quot;).getData().insert();</ClientEvent>
      <Property name="dataType">[dtCondition]</Property>
      <Property name="pageSize">20</Property>
    </DataSet>
    <DataSet id="dsCustomerCalStatus">
      <Property name="dataType">[dtCustomerCalStatus]</Property>
      <Property name="dataProvider">bmsAsynCalcuTaskController#query</Property>
      <Property name="pageSize">20</Property>
      <Property name="loadMode">manual</Property>
    </DataSet>
    <DataSet id="dsCustomerSubjectStatus">
      <Property name="dataType">[dtCustomerSubjectStatus]</Property>
      <Property name="dataProvider">bmsAsynCalcuTaskController#queryDetail</Property>
    </DataSet>
    <ToolBar>
      <DataPilot>
        <Property name="dataSet">dsCustomerCalStatus</Property>
        <Property name="itemCodes">pages,pageSize</Property>
      </DataPilot>
      <ToolBarButton id="btnQuery">
        <ClientEvent name="onClick">var data = view.get(&quot;#dsCondition.data&quot;).current;&#xD;
with (view.get(&quot;#dsCustomerCalStatus&quot;)){&#xD;
	set(&quot;parameter&quot;, data);&#xD;
	flushAsync();&#xD;
}</ClientEvent>
        <Property name="caption">查询</Property>
        <Property name="icon">url(>skin>common/icons.gif) -20px -40px</Property>
      </ToolBarButton>
    </ToolBar>
    <Container>
      <AutoForm id="queryForm">
        <Property name="createPrivateDataSet">true</Property>
        <Property name="cols">*,*,*,*</Property>
        <Property name="labelWidth">65</Property>
        <Property name="dataSet">dsCondition</Property>
        <AutoFormElement>
          <Property name="name">customerId</Property>
          <Property name="property">customerId</Property>
          <Property name="trigger">triggerCustomer</Property>
          <Editor/>
        </AutoFormElement>
        <AutoFormElement>
          <Property name="name">createYear</Property>
          <Property name="property">createYear</Property>
          <Editor/>
        </AutoFormElement>
        <AutoFormElement>
          <Property name="name">createMonth</Property>
          <Property name="property">createMonth</Property>
          <Editor/>
        </AutoFormElement>
        <AutoFormElement>
          <Property name="name">customerStatus</Property>
          <Property name="property">customerStatus</Property>
          <Editor/>
        </AutoFormElement>
      </AutoForm>
    </Container>
    <SplitPanel>
      <Property name="direction">top</Property>
      <Property name="position">280</Property>
      <MainControl>
        <Container>
          <DataGrid id="dataGridDetail">
            <Property name="dataSet">dsCustomerSubjectStatus</Property>
            <Property name="stretchColumnsMode">auto</Property>
            <Property name="readOnly">true</Property>
            <IndicatorColumn/>
            <RowNumColumn/>
            <DataColumn name="subjectName">
              <Property name="property">subjectName</Property>
            </DataColumn>
            <DataColumn>
              <Property name="name">feesType</Property>
              <Property name="property">feesType</Property>
            </DataColumn>
            <DataColumn>
              <ClientEvent name="onRenderCell">var customerStatus=arg.data.get(&quot;customerStatus&quot;);&#xD;
if(!isNullOrEmpty(customerStatus)){&#xD;
	if(customerStatus==&quot;3&quot;){&#xD;
		arg.dom.style.color = &quot;red&quot;;&#xD;
	}	&#xD;
}&#xD;
&#xD;
arg.processDefault = true;&#xD;
&#xD;
&#xD;
if(customerStatus==&quot;3&quot;){&#xD;
	arg.dom.style.color = &quot;red&quot;;&#xD;
}&#xD;
if(customerStatus==&quot;1&quot;){&#xD;
	arg.dom.style.color = &quot;green&quot;;&#xD;
}&#xD;
if(customerStatus==&quot;2&quot;){&#xD;
	arg.dom.style.color = &quot;yellow&quot;;&#xD;
}</ClientEvent>
              <Property name="property">customerStatus</Property>
              <Property name="name">customerStatus</Property>
            </DataColumn>
            <DataColumn name="feesCount">
              <Property name="property">feesCount</Property>
              <Property name="align">right</Property>
            </DataColumn>
            <DataColumn name="finishCount">
              <Property name="property">finishCount</Property>
              <Property name="align">right</Property>
            </DataColumn>
            <DataColumn name="contractMissCount">
              <ClientEvent name="onRenderCell">jQuery(arg.dom).empty();&#xD;
jQuery(arg.dom).xCreate({&#xD;
	tagName: &quot;A&quot;,&#xD;
	href: &quot;#&quot;,&#xD;
	//content:&quot;纠正&quot;,&#xD;
	onclick:function(){&#xD;
		//数据字典查询科目对应界面的URL&#xD;
		var subjectUrlAction = view.get(&quot;#subjectUrlAction&quot;);&#xD;
		//主的与明细的当前行&#xD;
		var dataMain = view.get(&quot;#dsCustomerCalStatus.data&quot;).current;&#xD;
		var dataDetail = view.get(&quot;#dsCustomerSubjectStatus.data&quot;).current;&#xD;
		var param=&quot;?customerId=&quot;+data.get(&quot;customerId&quot;)+&quot;&amp;creMonth=&quot;+creMonth+&quot;&amp;subjectCode=&quot;+dataDetail.get(&quot;subjectCode&quot;)+&quot;&amp;isCalculated=&quot;+&quot;3&quot;;&#xD;
		var data = {&#xD;
			code:dataDetail.get(&quot;subjectCode&quot;)&#xD;
		};&#xD;
		&#xD;
		//商品存储费单独判断，其余的费用科目走数据字典配置&#xD;
		if(&quot;wh_product_storage&quot; == dataDetail.get(&quot;subjectCode&quot;) &amp;&amp; &quot;ITEMS&quot; == dataDetail.get(&quot;feesType&quot;)){&#xD;
			window.top.openUrlInFrameTab(&quot;com.jiuyescm.bms.biz.storage.view.productsRepertory.d&quot;+param, &quot;商品库存_件&quot;, &quot;&quot;, true);&#xD;
		}else if(&quot;wh_product_storage&quot; == dataDetail.get(&quot;subjectCode&quot;) &amp;&amp; &quot;PALLETS&quot; == dataDetail.get(&quot;feesType&quot;)){&#xD;
			window.top.openUrlInFrameTab(&quot;com.jiuyescm.bms.biz.pallet.view.BizPalletInfo.d.d&quot;+param, &quot;托数管理&quot;, &quot;&quot;, true);&#xD;
		}else{&#xD;
			subjectUrlAction.set(&quot;parameter&quot;,data).execute(function(result){&#xD;
				console.log(result);&#xD;
				window.top.openUrlInFrameTab(result+param, &quot;&quot;, &quot;&quot;, true);&#xD;
			});&#xD;
		}&#xD;
	}&#xD;
});&#xD;
</ClientEvent>
              <Property name="property">contractMissCount</Property>
              <Property name="align">right</Property>
            </DataColumn>
            <DataColumn name="quoteMissCount">
              <Property name="property">quoteMissCount</Property>
              <Property name="align">right</Property>
            </DataColumn>
            <DataColumn>
              <Property name="property">uncalcuCount</Property>
              <Property name="name">uncalcuCount</Property>
              <Property name="align">right</Property>
            </DataColumn>
            <DataColumn name="sysErrorCount">
              <Property name="property">sysErrorCount</Property>
              <Property name="align">right</Property>
            </DataColumn>
            <DataColumn>
              <Property name="property">noExeCount</Property>
              <Property name="name">noExeCount</Property>
              <Property name="align">right</Property>
            </DataColumn>
          </DataGrid>
        </Container>
      </MainControl>
      <SideControl>
        <Container>
          <DataGrid id="dataGridMain">
            <ClientEvent name="onDataRowClick">var data = view.get(&quot;#dsCustomerCalStatus&quot;).getData();&#xD;
&#xD;
with (view.get(&quot;#dsCustomerSubjectStatus&quot;)){&#xD;
	//定义查询条件json结构&#xD;
	var queryJson={&#xD;
		creMonth:data.current.get(&quot;creMonth&quot;),&#xD;
		customerName:data.current.get(&quot;customerName&quot;)&#xD;
	};&#xD;
	set(&quot;parameter&quot;, queryJson);&#xD;
	flushAsync();&#xD;
}&#xD;
</ClientEvent>
            <Property name="dataSet">dsCustomerCalStatus</Property>
            <Property name="stretchColumnsMode">auto</Property>
            <Property name="readOnly">true</Property>
            <IndicatorColumn/>
            <RowNumColumn/>
            <DataColumn>
              <Property name="property">creMonth</Property>
              <Property name="name">creMonth</Property>
            </DataColumn>
            <DataColumn name="customerName">
              <Property name="property">customerName</Property>
            </DataColumn>
            <DataColumn>
              <ClientEvent name="onRenderCell">var customerStatus=arg.data.get(&quot;customerStatus&quot;);&#xD;
if(!isNullOrEmpty(customerStatus)){&#xD;
	if(customerStatus==&quot;3&quot;){&#xD;
		arg.dom.style.color = &quot;red&quot;;&#xD;
	}&#xD;
	if(calcStatus==&quot;1&quot;){&#xD;
		arg.dom.style.color = &quot;green&quot;;&#xD;
	}		&#xD;
}&#xD;
&#xD;
arg.processDefault = true;&#xD;
&#xD;
&#xD;
if(customerStatus==&quot;3&quot;){&#xD;
	arg.dom.style.color = &quot;red&quot;;&#xD;
}&#xD;
if(customerStatus==&quot;1&quot;){&#xD;
	arg.dom.style.color = &quot;green&quot;;&#xD;
}&#xD;
if(customerStatus==&quot;2&quot;){&#xD;
	arg.dom.style.color = &quot;yellow&quot;;&#xD;
}&#xD;
&#xD;
</ClientEvent>
              <Property name="property">customerStatus</Property>
              <Property name="name">customerStatus</Property>
            </DataColumn>
            <DataColumn name="subjectNum">
              <Property name="property">subjectNum</Property>
            </DataColumn>
          </DataGrid>
        </Container>
      </SideControl>
    </SplitPanel>
    <Trigger id="triggerCustomer">
      <ClientEvent name="onExecute">view.get(&quot;#panelCustomer&quot;).show();</ClientEvent>
    </Trigger>
    <FloatPanel id="panelCustomer">
      <ClientEvent name="onHide">var data = this.get(&quot;#viewSelectCustomer.userData&quot;);&#xD;
if (data != null) {&#xD;
	var entity = this.get(&quot;#dsCondition&quot;).getData(&quot;#&quot;);&#xD;
&#xD;
    entity.set({&#xD;
        customerId:data.get(&quot;customerid&quot;),&#xD;
		customerName:data.get(&quot;customername&quot;)&#xD;
    });&#xD;
}</ClientEvent>
      <Property name="caption">商家选择</Property>
      <Property name="center">true</Property>
      <Property name="closeable">true</Property>
      <Property name="contentOverflow">scroll</Property>
      <Property name="height">450</Property>
      <Property name="modal">true</Property>
      <Property name="width">800</Property>
      <Buttons/>
      <Children>
        <SubViewHolder id="viewSelectCustomer">
          <Property name="subView">com.jiuyescm.bms.base.customer.view.CustomerLookup</Property>
        </SubViewHolder>
      </Children>
      <Tools/>
    </FloatPanel>
    <AjaxAction id="subjectUrlAction">
      <Property name="service">bmsAsynCalcuTaskController#findUrlBySubjectCode</Property>
    </AjaxAction>
  </View>
</ViewConfig>
