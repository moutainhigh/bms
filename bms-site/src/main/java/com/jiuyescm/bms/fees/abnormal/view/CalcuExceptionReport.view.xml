	return;&#xD;
}&#xD;
var data = view.get(&quot;#dsFeesPayStorageQuery&quot;).getData().current;&#xD;
&#xD;
view.get(&quot;#feesPayDownAction&quot;).set(&quot;parameter&quot;,data).execute(&#xD;
