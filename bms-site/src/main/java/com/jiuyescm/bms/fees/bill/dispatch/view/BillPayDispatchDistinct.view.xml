        <ToolBarButton id="btnExport">
          <ClientEvent name="onClick">var dsFeesReceiveStorageEntity = view.get(&quot;#dsDeliveryDiffList&quot;).getData();&#xD;
var data = view.get(&quot;#dsCondition&quot;).getData().current;&#xD;
var count = dsFeesReceiveStorageEntity.entityCount;&#xD;
if(count == 0){&#xD;
	dorado.MessageBox.alert(&quot;没有数据可以导出！&quot;);&#xD;
	return;&#xD;
}&#xD;
var billNo=data.get(&quot;billNo&quot;);&#xD;
if(isNullOrEmpty(billNo)){&#xD;
	dorado.MessageBox.alert(&quot;账单编号不能为空！&quot;);&#xD;
	return;&#xD;
