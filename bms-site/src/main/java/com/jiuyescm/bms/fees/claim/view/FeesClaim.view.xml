		return;&#xD;
	}&#xD;
}&#xD;
&#xD;
//运单时间&#xD;
if(!isNullOrEmpty(waybillStartTime) &amp;&amp; isNullOrEmpty(waybillEndTime)){&#xD;
 	 dorado.MessageBox.alert(&quot;运单结束时间不能为空&quot;);&#xD;
     return;&#xD;
}&#xD;
if(isNullOrEmpty(waybillStartTime) &amp;&amp; !isNullOrEmpty(waybillEndTime)){&#xD;
 	 dorado.MessageBox.alert(&quot;运单起始时间不能为空&quot;);&#xD;
     return;&#xD;
}&#xD;
if(!isNullOrEmpty(waybillStartTime) &amp;&amp; !isNullOrEmpty(waybillEndTime)){&#xD;
