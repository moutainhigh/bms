        <Property name="label">数量上限</Property>
      </PropertyDef>
      <PropertyDef name="numLower">
        <Property name="label">数量下限</Property>
      </PropertyDef>
      <PropertyDef name="userDefine1">
        <Property name="label">扩展1</Property>
      </PropertyDef>
      <PropertyDef name="userDefine2">
        <Property name="label">扩展2</Property>
      </PropertyDef>
      <PropertyDef name="userDefine3">
        <Property name="label">扩展3</Property>
      </PropertyDef>
      <PropertyDef name="userDefine4">
        <Property name="label">扩展4</Property>
      </PropertyDef>
      <PropertyDef name="userDefine5">
        <Property name="label">扩展5</Property>
