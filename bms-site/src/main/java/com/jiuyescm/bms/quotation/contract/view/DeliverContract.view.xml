      <ToolBarButton id="btnQuery">
        <ClientEvent name="onClick">var tmpDataSet=view.get(&quot;#dsPriceContractInfoEntity&quot;);&#xD;
var tmpFormCondition=view.get(&quot;#formCondition&quot;);&#xD;
&#xD;
var cre=tmpFormCondition.get(&quot;entity&quot;).get(&quot;startTime&quot;);&#xD;
var end=tmpFormCondition.get(&quot;entity&quot;).get(&quot;endTime&quot;);&#xD;
if(!isNullOrEmpty(cre) &amp;&amp; !isNullOrEmpty(end)){&#xD;
 	if(cre.getTime()>end.getTime()){&#xD;
		dorado.MessageBox.alert(&quot;起始时间不能大于结束时间！&quot;);&#xD;
		return;&#xD;
	}&#xD;
}&#xD;
&#xD;
tmpDataSet.set(&quot;parameter&quot;, tmpFormCondition.get(&quot;entity&quot;)).flushAsync();&#xD;
//清空下面的签约服务&#xD;
var dispatchData=view.get(&quot;#dsContractDetailDispatch.data&quot;);&#xD;
if(!isNullOrEmpty(dispatchData)){&#xD;
	dispatchData.clear();&#xD;
}</ClientEvent>
        <Property name="caption">查询</Property>
        <Property name="icon">url(>skin>common/icons.gif) -20px -40px</Property>
      </ToolBarButton>
      <Separator/>
      <ToolBarButton id="btnAdd">
        <ClientEvent name="onClick">var datas = view.get(&quot;#dsPriceContractInfoEntity.data&quot;);&#xD;
datas.insert();&#xD;
view.get(&quot;#dialogContractEdit&quot;).show();</ClientEvent>
        <Property name="caption">新增合同</Property>
        <Property name="icon">url(>skin>common/icons.gif) -60px -120px</Property>
      </ToolBarButton>
      <Separator/>
      <ToolBarButton id="btnUpdate">
        <ClientEvent name="onClick">var data=view.get(&quot;#dsPriceContractInfoEntity.data&quot;);&#xD;
if(data.current){&#xD;
	var status=data.current.get(&quot;contractState&quot;);	&#xD;
	//审核过的无法修改合同&#xD;
	if(status=='1'){&#xD;
		dorado.MessageBox.alert(&quot;该合同已审核，无法编辑!&quot;);&#xD;
		return;&#xD;
	}	&#xD;
	//判断该合同是否已失效&#xD;
	var nowTime = new Date(); &#xD;
	&#xD;
	//获取失效日期&#xD;
	var expireDate=data.current.get(&quot;expireDate&quot;);&#xD;
	&#xD;
	var oDate2 = new Date(expireDate);&#xD;
	//已过期的合同不能修改&#xD;
	if(nowTime.getTime() > oDate2.getTime()){&#xD;
	    dorado.MessageBox.alert(&quot;该合同已过期，无法编辑!&quot;);&#xD;
		return;&#xD;
	}&#xD;
	&#xD;
	view.get(&quot;#dialogContractEdit&quot;).show();&#xD;
}else{&#xD;
	dorado.MessageBox.alert(&quot;请选择需要修改的记录!&quot;);&#xD;
}</ClientEvent>
        <Property name="caption">编辑合同</Property>
        <Property name="icon">url(>skin>common/icons.gif) -100px -120px</Property>
      </ToolBarButton>
      <Separator/>
      <ToolBarButton id="btnRemove">
        <ClientEvent name="onClick">var datas = view.get(&quot;#dsPriceContractInfoEntity.data&quot;);&#xD;
var currentData = datas.current;&#xD;
if(currentData){&#xD;
	dorado.MessageBox.confirm(&quot;是否确定要删除此条记录?&quot;,function(){&#xD;
		view.get(&quot;#removeContractAction&quot;).execute({&#xD;
			callback:function(success){&#xD;
				with (view.get(&quot;#dsPriceContractInfoEntity&quot;)){&#xD;
					flushAsync();&#xD;
				}&#xD;
			}&#xD;
		});&#xD;
	});&#xD;
}else{&#xD;
	dorado.MessageBox.alert(&quot;请首先选择要删除的记录!&quot;);&#xD;
}</ClientEvent>
        <Property name="caption">删除合同</Property>
        <Property name="icon">url(>skin>common/icons.gif) -80px -120px</Property>
      </ToolBarButton>
      <Separator/>
      <ToolBarButton id="btnAddDetail">
        <ClientEvent name="onClick">//对应的配送服务&#xD;
var dispatch=view.get(&quot;#dsContractDetailDispatch.data&quot;).current;&#xD;
&#xD;
//获取此时的审核状态&#xD;
var contractstate= view.get(&quot;#dsPriceContractInfoEntity&quot;).getData().current.get(&quot;contractState&quot;);&#xD;
if(contractstate=='1'){&#xD;
	dorado.MessageBox.alert(&quot;该合同已审核，无法进行编辑!&quot;);&#xD;
	return;&#xD;
}&#xD;
&#xD;
//获取此时的配送报价&#xD;
var dispatchData = view.get(&quot;#dsDispatchSystemCode.data&quot;);&#xD;
&#xD;
//先清空&#xD;
dispatchData.each(function(entity){&#xD;
	entity.set(&quot;isCheck&quot;,false);&#xD;
	entity.set(&quot;templateId&quot;,&quot;&quot;);&#xD;
});&#xD;
&#xD;
if(undefined==dispatch || dispatch==''){&#xD;
	var datas = view.get(&quot;#dsPriceContractItemEntity.data&quot;);&#xD;
	datas.insert();&#xD;
	view.get(&quot;#dialogContractItemEdit&quot;).show();	&#xD;
}else{&#xD;
	var datas = view.get(&quot;#dsPriceContractItemEntity.data&quot;);&#xD;
	datas.insert();&#xD;
	//dorado.MessageBox.alert(&quot;该合同已签约，无法继续签约!&quot;);&#xD;
	//进入编辑页面&#xD;
	//获取当前的合同id&#xD;
	var contract= view.get(&quot;#dsPriceContractInfoEntity&quot;).getData().current;&#xD;
	&#xD;
	var ajaxaction=view.get(&quot;#queryContractItemAction&quot;);&#xD;
	&#xD;
	//仓储&#xD;
	ajaxaction.set(&quot;parameter&quot;,contract).execute(function(result){&#xD;
		var strs= new Array(); //定义一数组 &#xD;
		result=result.substring(0,result.length-1);&#xD;
		strs=result.split(&quot;,&quot;); //字符分割 	&#xD;
	    dispatchData.each(function(entity){  &#xD;
		  for(var i=0;i&lt;strs.length;i++){&#xD;
			 	//传入进来的code值&#xD;
				if(strs[i].indexOf(&quot;&amp;&quot;)!=-1){&#xD;
					var dataCode=strs[i].substring(0,strs[i].indexOf(&quot;&amp;&quot;));&#xD;
					var datatemplate=strs[i].substring(strs[i].indexOf(&quot;&amp;&quot;)+1);	&#xD;
					//alert(&quot;此时的数据code&quot;+dataCode+&quot;自身的code&quot;+entity.get(&quot;code&quot;));			&#xD;
				 	if(entity.get(&quot;code&quot;)==dataCode){&#xD;
						//alert(&quot;进来&quot;);&#xD;
						entity.set(&quot;isCheck&quot;,true);&#xD;
			   			entity.set(&quot;templateId&quot;,datatemplate);	&#xD;
				}&#xD;
				&#xD;
				&#xD;
			}&#xD;
			&#xD;
		 }&#xD;
	});	&#xD;
		&#xD;
&#xD;
	});&#xD;
	view.get(&quot;#dialogContractItemEdit&quot;).show();	&#xD;
}&#xD;
&#xD;
</ClientEvent>
        <Property name="caption">签约服务</Property>
        <Property name="icon">url(>skin>common/icons.gif) -200px -120px</Property>
      </ToolBarButton>
      <Separator/>
      <ToolBarButton id="btnCheck">
        <ClientEvent name="onClick">//对应的配送服务&#xD;
var dispatch=view.get(&quot;#dsContractDetailDispatch.data&quot;).current;&#xD;
&#xD;
//获取此时的审核状态&#xD;
var contractstate= view.get(&quot;#dsPriceContractInfoEntity&quot;).getData().current.get(&quot;contractState&quot;);&#xD;
if(contractstate=='1'){&#xD;
	dorado.MessageBox.alert(&quot;该合同已审核，无法进行编辑!&quot;);&#xD;
	return;&#xD;
}&#xD;
&#xD;
&#xD;
if(undefined==dispatch || dispatch==''){&#xD;
	dorado.MessageBox.alert(&quot;该合同还未签约服务，请先签约再审核!&quot;);&#xD;
	return;&#xD;
}&#xD;
&#xD;
var data=view.get(&quot;#dsPriceContractInfoEntity.data&quot;);&#xD;
if(data.current){&#xD;
	&#xD;
	dorado.MessageBox.confirm(&quot;是否确定要审核该合同?&quot;,function(){&#xD;
		view.get(&quot;#checkContractAction&quot;).execute({&#xD;
			callback:function(success){&#xD;
				with (view.get(&quot;#dsPriceContractInfoEntity&quot;)){&#xD;
					flushAsync();&#xD;
				}&#xD;
			}&#xD;
		});&#xD;
	});&#xD;
}else{&#xD;
	dorado.MessageBox.alert(&quot;请选择需要修改的记录!&quot;);&#xD;
}</ClientEvent>
        <Property name="caption">审核</Property>
        <Property name="icon">url(>skin>common/icons.gif) -220px -120px</Property>
      </ToolBarButton>
      <Separator/>
      <ToolBarButton id="btnunaudit">
        <ClientEvent name="onClick">var data=view.get(&quot;#dsPriceContractInfoEntity.data&quot;).current;&#xD;
var contractstate= data.get(&quot;contractState&quot;);&#xD;
if(contractstate=='0'){&#xD;
	dorado.MessageBox.alert(&quot;该合同未审核，无需反审核!&quot;);&#xD;
