      <Buttons>
        <Button id="btnContractSave">
          <ClientEvent name="onClick">var entity = view.get(&quot;#formContractEdit&quot;).get(&quot;entity&quot;);	&#xD;
//获取生效日期&#xD;
var startTime=entity.get(&quot;startDate&quot;);&#xD;
//获取失效日期&#xD;
var expireDate=entity.get(&quot;expireDate&quot;);&#xD;
var oDate1 = new Date(startTime);&#xD;
var oDate2 = new Date(expireDate);&#xD;
