		//定义查询条件json结构&#xD;
		var queryJson={&#xD;
			taskName:taskName,&#xD;
			year:condition.get(&quot;year&quot;),&#xD;
			month:condition.get(&quot;month&quot;),&#xD;
			customerId:condition.get(&quot;customerId&quot;)&#xD;
		}; &#xD;
		set(&quot;parameter&quot;, queryJson);&#xD;
		flushAsync();&#xD;
	}&#xD;
});&#xD;
</ClientEvent>
