        <ToolBarButton id="btnDerateFee">
          <ClientEvent name="onClick">var tableData=view.get(&quot;#distributionDetailGrid&quot;).get(&quot;selection&quot;);&#xD;
if(tableData.length>0){&#xD;
	var dataSet=view.get(&quot;#dsFeesReceiveDistributionEdit&quot;);&#xD;
	dataSet.clear();&#xD;
	tableData.each(function(item){&#xD;
		dataSet.insert({&#xD;
			id:item.get(&quot;id&quot;),&#xD;
			feesNo:item.get(&quot;feesNo&quot;),&#xD;
			derateAmount:item.get(&quot;derateAmount&quot;),&#xD;
			outstockNo:item.get(&quot;outstockNo&quot;),&#xD;
			amount:item.get(&quot;amount&quot;)&#xD;
		});&#xD;
	});&#xD;
	view.get(&quot;#dialogDerate&quot;).show();&#xD;
}else{&#xD;
	dorado.MessageBox.alert(&quot;请选择需要修改的记录!&quot;);&#xD;
}&#xD;
</ClientEvent>
