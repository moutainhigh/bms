        <ClientEvent name="onClick">var data = view.get(&quot;#dsReportSellerProfitQuery.data&quot;).current;&#xD;
var year=data.get(&quot;reportYear&quot;);&#xD;
if(!year){&#xD;
	year=&quot;&quot;;&#xD;
	dorado.MessageBox.alert(&quot;请选择年份！&quot;);&#xD;
	return;&#xD;
}else{&#xD;
	year=year+&quot;年&quot;;&#xD;
}&#xD;
this.id(&quot;lblTitle&quot;).set(&quot;text&quot;,year+&quot;商家利润报表&quot;);&#xD;
with (view.get(&quot;#dsReportSellerProfitEntity&quot;)){&#xD;
	set(&quot;parameter&quot;, data);&#xD;
	flush();&#xD;
}&#xD;
var dataSet=view.get(&quot;#dsReportSellerProfitEntity&quot;).getData();&#xD;
var table=&quot;&quot;;&#xD;
var sellerId=&quot;&quot;;&#xD;
table+=&quot;&lt;table class='reportTable'>&quot;;&#xD;
table+=&quot;&lt;thead>&quot;;&#xD;
table+=&quot;&lt;tr>&quot;;&#xD;
