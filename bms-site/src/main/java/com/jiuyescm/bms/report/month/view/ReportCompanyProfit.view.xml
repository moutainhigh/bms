      <PropertyDef name="amountSum">
        <Property name="label">总计</Property>
      </PropertyDef>
      <PropertyDef name="reportYear">
        <Property name="label">年份</Property>
      </PropertyDef>
      <PropertyDef name="createTime">
        <Property name="label">创建时间</Property>
        <Property name="dataType">DateTime</Property>
      </PropertyDef>
      <PropertyDef name="modifyTime">
        <Property name="label">修改时间</Property>
        <Property name="dataType">DateTime</Property>
      </PropertyDef>
    </DataType>
    <DataType name="dtReportCompanyProfitTypeQuery">
      <PropertyDef name="reportYear">
        <Property name="label">年份</Property>
      </PropertyDef>
      <PropertyDef name="IsCheck">
        <Property name="label">含总部管理费</Property>
        <Property name="defaultValue">False</Property>
        <Property name="dataType">boolean</Property>
      </PropertyDef>
    </DataType>
  </Model>
  <View>
    <ClientEvent name="onReady"> var date=new Date;&#xD;
 var year=date.getFullYear(); &#xD;
 view.get(&quot;#formCondition.entity&quot;).set(&quot;reportYear&quot;,year);&#xD;
&#xD;
</ClientEvent>
    <DataSet id="dsReportCompanyProfitEntity">
      <Property name="dataProvider">reportCompanyProfitController#query</Property>
      <Property name="readOnly">false</Property>
      <Property name="dataType">[dtCondition]</Property>
      <Property name="loadMode">manual</Property>
    </DataSet>
    <DataSet id="dsReportCompanyProfitQuery">
      <ClientEvent name="onCreate">view.get(&quot;#dsReportCompanyProfitQuery&quot;).getData().insert();&#xD;
		  </ClientEvent>
      <Property name="dataType">[dtReportCompanyProfitTypeQuery]</Property>
    </DataSet>
    <AutoForm id="formCondition">
      <Property name="dataType">dtReportCompanyProfitTypeQuery</Property>
      <Property name="createPrivateDataSet">true</Property>
      <Property name="cols">220,150,*</Property>
      <Property name="labelWidth">100</Property>
      <Property name="dataSet">dsReportCompanyProfitQuery</Property>
      <AutoFormElement>
        <Property name="name">reportYear</Property>
        <Property name="property">reportYear</Property>
        <Property name="labelWidth">60</Property>
        <Editor/>
      </AutoFormElement>
      <AutoFormElement>
        <Property name="editorType">CheckBox</Property>
        <Property name="property">IsCheck</Property>
        <Property name="name">IsCheck</Property>
        <Editor/>
      </AutoFormElement>
      <Button id="btnQuery">
        <ClientEvent name="onClick">&#xD;
var data = view.get(&quot;#dsReportCompanyProfitQuery.data&quot;).current;&#xD;
&#xD;
var year=data.get(&quot;reportYear&quot;);&#xD;
if(!year){&#xD;
	year=&quot;&quot;;&#xD;
}else{&#xD;
	year=year+&quot;年&quot;;&#xD;
}&#xD;
this.id(&quot;lblTitle&quot;).set(&quot;text&quot;,year+&quot;公司利润报表&quot;);&#xD;
with (view.get(&quot;#dsReportCompanyProfitEntity&quot;)){&#xD;
	set(&quot;parameter&quot;, data);&#xD;
	flushAsync();&#xD;
}		</ClientEvent>
        <Property name="caption">查询</Property>
        <Property name="icon">url(>skin>common/icons.gif) -20px -40px</Property>
      </Button>
      <AutoFormElement layoutConstraint="colSpan:3;align:center">
        <Property name="labelAlign">center</Property>
        <Property name="labelPosition">top</Property>
        <Property name="width">100%</Property>
        <Property name="style">
          <Property name="key"></Property>
        </Property>
        <Editor>
          <Container>
            <Label id="lblTitle">
              <Property name="text">公司利润报表</Property>
              <Property name="style">
                <Property name="font-size">20px</Property>
                <Property name="font-weight">bold</Property>
                <Property name="font-family">微软雅黑</Property>
              </Property>
            </Label>
          </Container>
        </Editor>
      </AutoFormElement>
    </AutoForm>
    <DataGrid>
      <Property name="dataSet">dsReportCompanyProfitEntity</Property>
      <Property name="stretchColumnsMode">auto</Property>
      <Property name="readOnly">true</Property>
      <Property name="showFooter">false</Property>
      <Property name="groupProperty">reportYear</Property>
      <Property name="groupOnSort">false</Property>
      <RowNumColumn/>
      <DataColumn name="reportYear">
        <Property name="property">reportYear</Property>
        <Property name="width">80</Property>
      </DataColumn>
      <DataColumn name="feesType">
        <Property name="property">feesType</Property>
        <Property name="width">80</Property>
      </DataColumn>
