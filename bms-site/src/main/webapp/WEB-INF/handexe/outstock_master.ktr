    <sql>select &#xd;&#xa;oi.orderno as outstock_no,&#xd;&#xa;oi.reference as external_no, &#xd;&#xa;oex.expressnum as waybill_no,&#xd;&#xa;oi.originwarehouseid as warehouse_code,&#xd;&#xa;wh.warehousename as warehouse_name,&#xd;&#xa;oi.customerid as customerid,	&#xd;&#xa;ct.customername customer_name,&#xd;&#xa;oi.min_tempraturetype as temperature_type_code,&#xd;&#xa;oi.min_tempraturetype as temperature_type_name,&#xd;&#xa;oi.ordertypeid as bill_type_code,&#xd;&#xa;f.name as bill_type_name,&#xd;&#xa;tio.b2bflag as b2b_flag,&#xd;&#xa;ow.weight as total_weight,&#xd;&#xa;0 as resize_weight,      &#xd;&#xa;oi.totalqty as total_quantity,    &#xd;&#xa;count&#x28;oit.productid&#x29; as total_varieties,&#xd;&#xa;&#x27;0&#x27; as is_calculated ,&#xd;&#xa;&#x27;&#x27; as fees_no,&#xd;&#xa;0 as del_flag,&#xd;&#xa;oex.cretime create_time, &#xd;&#xa;current_timestamp&#x28;&#x29; as write_time&#xd;&#xa;  from oms_orderinfo oi&#xd;&#xa;  join oms_tio tio on tio.orderinfoid &#x3d; oi.orderinfoid&#xd;&#xa;  join oms_orderinfoitem oit on oit.orderinfoid &#x3d; oi.orderinfoid&#xd;&#xa;  join oms_orderexpressnum oex on oex.crossdoid &#x3d; oi.orderno&#xd;&#xa;  join oms_orderweight ow on ow.expressnum &#x3d; oex.expressnum&#xd;&#xa;  join oms_waybill_state ows on ows.order_no &#x3d; oi.orderno&#xd;&#xa;  left join system_code sc on ows.small_status &#x3d; sc.extattr1&#xd;&#xa;  left join pub_warehouse wh on wh.warehouseid &#x3d; oi.originwarehouseid&#xd;&#xa;  left join pub_customer ct on ct.customerid &#x3d; oi.customerid&#xd;&#xa;  left join pub_deliver de on de.deliverid &#x3d; oex.deliverid&#xd;&#xa;	left join pub_billtype f on oi.ordertypeid &#x3d; f.ordertypeid&#xd;&#xa;where oi.customerid &#x3d; &#x3f;&#xd;&#xa;and oex.cretime &#x3e;&#x3d; &#x3f;&#xd;&#xa;and oex.cretime &#x3c; &#x3f;&#xd;&#xa;and oi.exceptionflag &#x3d; 0&#xd;&#xa;group by oi.orderno,oi.reference,oex.expressnum&#xd;&#xa;order by create_time&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
